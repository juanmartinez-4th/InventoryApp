<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://dozer.sourceforge.net http://dozer.sourceforge.net/schema/beanmapping.xsd">
	
	<configuration>
		<stop-on-errors>true</stop-on-errors>
		<date-format>MM/dd/yyyy HH:mm</date-format>
		<wildcard>true</wildcard>
	</configuration>
	
	<mapping>
        <class-a>mx.com.mindbits.argos.inventory.entity.Category</class-a>
        <class-b>mx.com.mindbits.argos.inventory.vo.CategoryVO</class-b>
    </mapping>
	
	<mapping>
		<class-a>mx.com.mindbits.argos.inventory.entity.Item</class-a>
		<class-b>mx.com.mindbits.argos.inventory.vo.ItemVO</class-b>
	</mapping>
	
	<mapping>
        <class-a>mx.com.mindbits.argos.inventory.entity.ItemClassification</class-a>
        <class-b>mx.com.mindbits.argos.inventory.vo.ItemClassificationVO</class-b>
    </mapping>
	
	<mapping>
        <class-a>mx.com.mindbits.argos.inventory.entity.ItemLocation</class-a>
        <class-b>mx.com.mindbits.argos.inventory.vo.ItemLocationVO</class-b>
    </mapping>
	
	<mapping>
        <class-a>mx.com.mindbits.argos.inventory.entity.ItemPicture</class-a>
        <class-b>mx.com.mindbits.argos.inventory.vo.ItemPictureVO</class-b>
    </mapping>
	
	<mapping>
        <class-a>mx.com.mindbits.argos.inventory.entity.Transaction</class-a>
        <class-b>mx.com.mindbits.argos.inventory.vo.TransactionVO</class-b>
    </mapping>
    
    <mapping>
        <class-a>mx.com.mindbits.argos.inventory.entity.UnitOfMeasure</class-a>
        <class-b>mx.com.mindbits.argos.inventory.vo.UnitOfMeasureVO</class-b>
    </mapping>
	
</mappings> 